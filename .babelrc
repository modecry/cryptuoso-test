{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "src/components",
          "core": "src/core",
          "graphQL": "src/graphql",
          "pages": "src/pages"
        },
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ]
  ]
}
