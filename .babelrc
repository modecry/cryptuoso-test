{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "src/components",
          "core": "src/core",
          "graphQL": "src/graphql",
          "pages": "src/pages",
          "enhancers": "src/enhancers",
          "constants": "src/core/constants",
          "services": "src/core/services",
          "utils": "src/core/utils"
        },
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ]
  ]
}
